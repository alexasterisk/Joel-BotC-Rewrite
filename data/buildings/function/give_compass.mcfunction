# Player 1
$execute as @a[scores={Player=-1}] if data entity @s Inventory[{Slot:-106b}] run give @s compass[lodestone_tracker={target:{dimension:"minecraft:overworld",pos:[I;$(marker_house_1_x),$(marker_house_1_y),$(marker_house_1_z)]},tracked:false},custom_name='{"color":"gold","italic":true,"text":"House Locator"}',lore=['"Somehow it magically points you"','"in the direction of your home..."']] 1
$execute as @a[scores={Player=-1}] unless data entity @s Inventory[{Slot:-106b}] run item replace entity @s weapon.offhand with compass[lodestone_tracker={target:{dimension:"minecraft:overworld",pos:[I;$(marker_house_1_x),$(marker_house_1_y),$(marker_house_1_z)]},tracked:false},custom_name='{"color":"gold","italic":true,"text":"House Locator"}',lore=['"Somehow it magically points you"','"in the direction of your home..."']] 1

